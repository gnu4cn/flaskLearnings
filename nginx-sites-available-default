server {
    listen 80;

    #server_name app.local:80;
    server_name app.local;
    
    # Things about gzip
    gzip on;  
    gzip_comp_level 7;  
    gzip_min_length  1100; #需要压缩的最小长度  
    gzip_buffers    4 8k;  
    gzip_types      text/plain application/javascript text/css text/xml image/jpeg image/git image/png; #指定需要压缩的文件类型  
    output_buffers  1 32k; 
    postpone_output  1460;
    gzip_vary on;
    gzip_disable "MSIE [1-6].";
    
    # To uwsgi-flask
    location / {
        include uwsgi_params;
        uwsgi_pass 127.0.0.1:8001;
        uwsgi_param UWSGI_PYHOME /home/unisko/flask/venv;
    }
    
    # angularJS partials
    location /partials/ {
        alias /home/unisko/flask/demos/restful/partials/;
    }

}
